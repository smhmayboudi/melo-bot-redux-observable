    "lint:fix": "eslint --ext .js,.ts --fix ./src ./types; exit 0",
    "remotedev": "remotedev --hostname=127.0.0.1 --port=8000",
    "start:app": "./start.app.sh",
    "start:polling": "./start.polling.sh",
    "test": "./test.sh",
    "test:watch": "./test.watch.sh",
    "type-check": "tsc --noEmit --project tsconfig.json; exit 0",
    "type-check:watch": "tsc --noEmit --project tsconfig.json --watch; exit 0"
