{
  "collectCoverage": true,
  "globalSetup": "./db/setup.js",
  "globalTeardown": "./db/teardown.js",
  "moduleFileExtensions": ["js", "ts"],
  "roots": [ "<rootDir>/src" ],
  "testEnvironment": "./db/environment.js",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(t|j)s$",
  "transform": {
    "^.+\\.ts$": "ts-jest"
  }
}
