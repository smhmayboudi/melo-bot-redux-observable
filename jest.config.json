{
  "collectCoverage": true,
  "globalSetup": "./db/setup.js",
  "globalTeardown": "./db/teardown.js",
  "moduleFileExtensions": ["ts", "js", "json"],
  "roots": [ "<rootDir>/src" ],
  "testEnvironment": "./db/environment.js",
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  }
}