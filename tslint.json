{
  "extends": [
    "tslint:all",
    "tslint-config-prettier"
  ],
  "rules": {
    "completed-docs": false,
    "no-any": false,
    "no-inferrable-types": false,
    "no-submodule-imports": false,
    "no-unsafe-any": false,
    "no-use-before-declare": false,
    "prettier": true,
    "variable-name": [true, "allow-leading-underscore"]
  },
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "./src/lib/lib/*"
    ]
  }
}