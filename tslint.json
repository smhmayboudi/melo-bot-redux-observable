{
  "extends": [
    "tslint:all"
  ],
  "rules": {
    "completed-docs": false,
    "no-any": false,
    "no-inferrable-types": false,
    "no-submodule-imports": false,
    "no-unsafe-any": false,
    "no-use-before-declare": false,
    "variable-name": {"options": ["allow-leading-underscore"]}
  },
  "linterOptions": {
    "exclude": [
      "./src/lib/lib/*"
    ]
  }
}