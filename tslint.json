{
  "extends": [
    "tslint:all"
  ],
  "rules": {
    "completed-docs": false,
    "no-any": false,
    "no-inferrable-types": false,
    "no-submodule-imports": false,
    "no-unused-variable": [true, {"ignore-pattern": "(action|dependencies|err|middlewareAPI|request|state\\$)"}],
    "no-unsafe-any": false,
    "no-use-before-declare": false
  },
  "linterOptions": {
    "exclude": [
      "./src/lib/lib/*"
    ]
  }
}